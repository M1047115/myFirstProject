package multithreading;

public class Main {

}

class Fibonacci extends Thread
{
	int x,fibo;
	public Fibonacci(int x)
	{
		this.x=x;
	}
	public void run()
	{
		if(x==0)
			fibo=0;
		else if(x<2)
			fibo=1;
		else
		{
			Fibonacci ob=new Fibonacci(x-1);
			Fibonacci ob1=new Fibonacci(x-2);
			ob.start();
			ob1.start();
			try {
				ob.join();
			ob1.join();
			}
			catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}
	}
}
